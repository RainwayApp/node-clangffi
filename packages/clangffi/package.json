{
  "name": "clangffi",
  "version": "2.0.0",
  "description": "Generate Typescript FFI bindings for C/C++ libraries using libclang and ffi-napi",
  "type": "module",
  "exports": "./dist/lib/index.js",
  "bin": {
    "clangffi": "dist/bin/clangffi.js"
  },
  "scripts": {
    "build": "tsc -p .",
    "prepublishOnly": "npm run build",
    "test": "jest"
  },
  "jest": {
    "transformIgnorePatterns": [
      "<rootDir>/node_modules/(?!matcher)"
    ],
    "testPathIgnorePatterns": [
      "/node_modules/",
      "<rootDir>/dist/"
    ]
  },
  "keywords": [
    "c++",
    "cpp",
    "c",
    "ffi",
    "generator"
  ],
  "author": "Ben Greenier <ben@rainway.com>",
  "license": "MIT",
  "devDependencies": {
    "@babel/core": "^7.16.7",
    "@babel/preset-env": "^7.16.7",
    "@babel/preset-typescript": "^7.16.7",
    "@types/debug": "^4.1.7",
    "@types/jest": "^27.4.0",
    "@types/node": "^17.0.6",
    "@types/prettier": "^2.4.2",
    "@types/yargs": "^17.0.8",
    "babel-jest": "^27.4.5",
    "jest": "^27.4.5",
    "matcher": "^5.0.0",
    "typescript": "^4.5.4"
  },
  "dependencies": {
    "debug": "^4.3.3",
    "libclang-bindings": "^1.1.0",
    "prettier": "^2.5.1",
    "yargs": "^17.3.1"
  },
  "engines": {
    "node": ">=17.x"
  }
}
